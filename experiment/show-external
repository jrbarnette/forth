#!/bin/sh

SOURCE=$1
(
    sed '
	s/( [^)]*)//
	s/\\ .*$//
	s/\." [^"]*"/." /
	s/	/ /g
	s/^ *//
	s/ *$//
	s/ [0-9][0-9]* / /
	s/ -[0-9][0-9]* / /
	s/ [0-9][0-9]*$//
	s/ -[0-9][0-9]*$//
	s/^[0-9][0-9]* //
	s/^-[0-9][0-9]* //
	s/  */\
/g
	/^$/ d
    ' $SOURCE | sort -u
    sed '
	/^:/ !d
	s/^: //
	s/ .*$//
    ' $SOURCE | sort -u
) | sort | uniq -u
